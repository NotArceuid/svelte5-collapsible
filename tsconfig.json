{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "rewriteRelativeImportExtensions": true,
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "bundler"
  },
  "include": [
    "src/lib/**/*.ts",
    "src/lib/**/*.svelte",
    "src/lib/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
